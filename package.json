{
  "name": "flow-field",
  "version": "1.0.0",
  "description": "Project to quickly get something working in [p5.js](https://p5js.org/) and [typescript](https://www.typescriptlang.org/)",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p start-compile start-run",
    "start-compile": "tsc --watch",
    "start-run": "browser-sync start --server -w",
    "build": "tsc",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "homepage": "https://github.com/neonima/flow-field",
  "dependencies": {
    "set-value": ">=2.0.1",
    "mixin-deep": ">=1.3.2"
  },
  "devDependencies": {
    "@types/node": "^10.0.3",
    "@types/p5": "^0.9.0",
    "browser-sync": ">=2.26.7",
    "gts": "^1.1.0",
    "lodash": ">=4.17.15",
    "npm-run-all": ">=4.1.5",
    "typescript": "~3.6.3"
  }
}
